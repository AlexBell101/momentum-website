{
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "installCommand": "npm install",
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://datakarma.ai/momentum/$1",
      "has": [
        {
          "type": "host",
          "value": "momentum.datakarma.ai"
        }
      ],
      "statusCode": 301
    }
  ]
}
