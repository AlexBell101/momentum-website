{
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/",
      "destination": "/eventkarma",
      "has": [
        {
          "type": "host",
          "value": "eventkarma.ai"
        }
      ]
    },
    {
      "source": "/",
      "destination": "/eventkarma",
      "has": [
        {
          "type": "host",
          "value": "www.eventkarma.ai"
        }
      ]
    },
    {
      "source": "/:path*",
      "destination": "/eventkarma/:path*",
      "has": [
        {
          "type": "host",
          "value": "eventkarma.ai"
        }
      ]
    },
    {
      "source": "/:path*",
      "destination": "/eventkarma/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.eventkarma.ai"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://www.datakarma.ai/eventkarma/$1",
      "has": [
        {
          "type": "host",
          "value": "momentum.datakarma.ai"
        }
      ],
      "statusCode": 301
    },
    {
      "source": "/momentum",
      "destination": "/eventkarma",
      "statusCode": 301
    },
    {
      "source": "/momentum/:path*",
      "destination": "/eventkarma/:path*",
      "statusCode": 301
    }
  ]
}
